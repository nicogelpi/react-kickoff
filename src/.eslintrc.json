{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "protractor": true,
    "mocha": true
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-warning-comments": 1,
    "no-unused-vars": 1,
    "no-console": 1
  }
}
